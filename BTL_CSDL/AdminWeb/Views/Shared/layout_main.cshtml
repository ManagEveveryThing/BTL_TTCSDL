<!DOCTYPE html>

<html>
<head>
    <!-- Required meta tags-->
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="au theme template">
    <meta name="author" content="Hau Nguyen">
    <meta name="keywords" content="au theme template">


    <!-- Fontfaces CSS-->
    <link href="~/Contents/css/font-face.css" rel="stylesheet" media="all">
    <link href="~/Contents/vendor/font-awesome-4.7/css/font-awesome.min.css" rel="stylesheet" media="all">
    <link href="~/Contents/vendor/font-awesome-5/css/fontawesome-all.min.css" rel="stylesheet" media="all">
    <link href="~/Contents/vendor/mdi-font/css/material-design-iconic-font.min.css" rel="stylesheet" media="all">

    <!-- Bootstrap CSS-->
    <link href="~/Contents/vendor/bootstrap-4.1/bootstrap.min.css" rel="stylesheet" media="all">

    <!-- Vendor CSS-->
    <link href="~/Contents/vendor/animsition/animsition.min.css" rel="stylesheet" media="all">
    <link href="~/Contents/vendor/bootstrap-progressbar/bootstrap-progressbar-3.3.4.min.css" rel="stylesheet" media="all">
    <link href="~/Contents/vendor/wow/animate.css" rel="stylesheet" media="all">
    <link href="~/Contents/vendor/css-hamburgers/hamburgers.min.css" rel="stylesheet" media="all">
    <link href="~/Contents/vendor/slick/slick.css" rel="stylesheet" media="all">
    <link href="~/Contents/vendor/select2/select2.min.css" rel="stylesheet" media="all">
    <link href="~/Contents/vendor/perfect-scrollbar/perfect-scrollbar.css" rel="stylesheet" media="all">

    <!-- Main CSS-->
    <link href="~/Contents/css/theme.css" rel="stylesheet" media="all">
    <style>
        
    </style>
</head>

<body class="animsition">
    <div class="page-wrapper">
        <!-- HEADER MOBILE-->
        <header class="header-mobile d-block d-lg-none">
            <div class="header-mobile__bar">
                <div class="container-fluid">
                    <div class="header-mobile-inner">
                        <a class="logo" href="@Url.Action("Index", "Home")">
                            <img src="~/Contents/images/icon/mta_icon.ico"  style="width:75px" alt="MTA" />
                        </a>
                        <button class="hamburger hamburger--slider" type="button">
                            <span class="hamburger-box">
                                <span class="hamburger-inner"></span>
                            </span>
                        </button>
                    </div>
                </div>
            </div>
            <nav class="navbar-mobile">
                <div class="container-fluid">
                    <ul class="navbar-mobile__list list-unstyled">
                        <li class="has-sub">
                            <a class="js-arrow" href="@Url.Action("Index", "Home")">
                                <i class="fas fa-tachometer-alt"></i>Dashboard
                            </a>
                        </li>
                        <li>
                            @*<i class="fas fa-table"></i>@Html.ActionLink("Table", "Table", "Home")*@
                            <a href="@Url.Action("Table", "Home")">
                                <i class="fas fa-table"></i>Tables
                            </a>
                        </li>
                        @*<li>
                            <a href="calendar.html">
                                <i class="fas fa-calendar-alt"></i>Calendar
                            </a>
                        </li>*@
                    </ul>
                </div>
            </nav>
        </header>
        <!-- END HEADER MOBILE-->
        <!-- MENU SIDEBAR-->
        <aside class="menu-sidebar d-none d-lg-block">
            <div class="logo">
                <a href="@Url.Action("Index", "Home")">
                    <img src="~/Contents/images/icon/mta_icon.ico" style="width:75px" alt="MTA" />
                </a>
            </div>
            <div class="menu-sidebar__content js-scrollbar1">
                <nav class="navbar-sidebar">
                    <ul class="list-unstyled navbar__list">
                        <li class="active has-sub">
                            <a class="js-arrow" href="@Url.Action("Index", "Home")">
                                <i class="fas fa-tachometer-alt"></i>Dashboard
                            </a>
                        </li>
                        <li>
                            <a href="@Url.Action("Table", "Home")">
                                <i class="fas fa-table"></i>Tables
                            </a>
                        </li>
                        <li>
                            <a href="@Url.Action("FindingStudent", "Home")">
                                <i class="fas fa-table"></i>Finding Student
                            </a>
                        </li>
                        <li>
                            <a href="@Url.Action("FindingTKT", "Home")">
                                <i class="fas fa-table"></i>Finding Ending Test
                            </a>
                        </li>
                        <li>
                            <a href="@Url.Action("Table", "Home")">
                                <i class="fas fa-table"></i>Fliter
                            </a>
                        </li>
                        <li>
                            <a href="@Url.Action("Table", "Home")">
                                <i class="fas fa-table"></i>Statics
                            </a>
                        </li>
                        @*<li>
                            <a href="calendar.html">
                                <i class="fas fa-calendar-alt"></i>Calendar
                            </a>
                        </li>*@
                    </ul>
                </nav>
            </div>
        </aside>
        <!-- END MENU SIDEBAR-->
        <!-- PAGE CONTAINER-->
        <div class="page-container">
            <!-- HEADER DESKTOP-->
            <header class="header-desktop">
                <div class="section__content section__content--p30">
                    <div class="container-fluid">
                        <div class="header-wrap">
                            <form class="form-header" action="" method="POST">
                                <input class="au-input au-input--xl" type="text" name="search" placeholder="Search for datas &amp; reports..." />
                                <button class="au-btn--submit" type="submit">
                                    <i class="zmdi zmdi-search"></i>
                                </button>
                            </form>
                           
                        </div>
                    </div>
                </div>
            </header>
            <!-- HEADER DESKTOP-->
            @RenderBody()
            <!-- MAIN CONTENT-->
            <!-- END MAIN CONTENT-->
            <!-- END PAGE CONTAINER-->
        </div>

    </div>

    <!-- Jquery JS-->
    <script src="~/Contents/vendor/jquery-3.2.1.min.js"></script>
    <!-- Bootstrap JS-->
    <script src="~/Contents/vendor/bootstrap-4.1/popper.min.js"></script>
    <script src="~/Contents/vendor/bootstrap-4.1/bootstrap.min.js"></script>
    <!-- Vendor JS       -->
    <script src="~/Contents/vendor/slick/slick.min.js">
    </script>
    <script src="~/Contents/vendor/wow/wow.min.js"></script>
    <script src="~/Contents/vendor/animsition/animsition.min.js"></script>
    <script src="~/Contents/vendor/bootstrap-progressbar/bootstrap-progressbar.min.js">
    </script>
    <script src="~/Contents/vendor/counter-up/jquery.waypoints.min.js"></script>
    <script src="~/Contents/vendor/counter-up/jquery.counterup.min.js">
    </script>
    <script src="~/Contents/vendor/circle-progress/circle-progress.min.js"></script>
    <script src="~/Contents/vendor/perfect-scrollbar/perfect-scrollbar.js"></script>
    <script src="~/Contents/vendor/chartjs/Chart.bundle.min.js"></script>
    <script src="~/Contents/vendor/select2/select2.min.js">
    </script>

    <!-- Main JS-->
    <script src="~/Contents/js/main.js"></script>
    <script src="~/Contents/js/homeController.js"></script>

</body>
</html>
